version: '3.9'

services:
  app-fastapi:
    build: .
    command: uvicorn app.main:app --host 0.0.0.0 --ssl-keyfile app/privkey.pem --ssl-
certfile app/fullchain.pem
    ports:
      - "8888:8000"
    networks:
      - app-fastapi
      - database_postgres-database

networks:
  app-fastapi:
    driver: bridge

  database_postgres-database:
    external: true
